            <button type="button" class="btn btn-danger btn-xs rmv" style="width:25px;border-radius:50%;" disabled="true">-</button>
        </label>
        <input type="text" class="form-control" placeholder="XXX.XXX.XXX-XX" id="form-doc1" name="form-doc1">
    </div>
</div>
