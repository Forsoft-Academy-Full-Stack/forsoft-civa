        <label for="tipo-doc1">Tipo do Documento:</label>
        <select class="form-control select2 select2-hidden-accessible" id="tipo-doc1" name="tipo-doc1" style="width: 100%;" tabindex="-1" aria-hidden="true">
            <option value="" selected disabled>Selecionar Tipo de Documento</option>
            <option value="1" >Passaporte</option>
            <option value="2">Identidade</option>
            <option value="3">CPF</option>
        </select>
    </div>
    <div class="form-group col-xl-4">
        <label for="doc1" style="display:flex;align-items:center;gap:2px;">Nome do Documento</label>
